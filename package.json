{
  "name": "console-colors",
  "version": "0.0.1",
  "description": "A simple package to print color in the console",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "prepare": "NODE_ENV=production npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "color",
    "consola",
    "print",
    "ANSII"
  ],
  "author": "David Arenas <davejs136@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.6.0",
    "concurrently": "^5.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
